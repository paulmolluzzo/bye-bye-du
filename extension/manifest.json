{
  "manifest_version": 2,

  "name": "Bye Baidu",
  "short_name": "Bye Baidu",
  "description": "Remove Baidu tracking and social network scripts.",
  "version": "0.2.0",

  "permissions": [
      "activeTab"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],

  "icons": {
      "128": "icon_128.png",
      "16": "icon_16.png",
      "48": "icon_48.png"
   }
}
